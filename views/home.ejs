<% include partials/header.ejs %>




<%  if(currentUser !== undefined)  {        %>

	<%	if(currentUser.role === "seller" && currentUser.request !== "pending") {   %>
			<div class="container">
				<div>

					<a href="/auctions" class="sellerPicture">
						<h3 class="sellerPictureTitle">My auctions</h3>
						<img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcT0izdhYBGg--nU2VvKHWNwPCRPzxyG_TITnJEMEEWwDzn3VGrFhQ" style="width:100%; border: 1px solid black" class="img-thumbnail rounded-circle float-left" >
